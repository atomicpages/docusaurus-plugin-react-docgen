{
  "name": "docusaurus-plugin-react-docgen",
  "version": "0.0.3",
  "description": "A small plugin that integrates react-docgen with docusaurus 2.x",
  "keywords": [
    "docusaurus",
    "plugin",
    "react-docgen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/atomicpages/docusaurus-plugin-react-docgen"
  },
  "license": "MIT",
  "author": "Dennis Thompson",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint src --ext=js,ts,tsx",
    "prebuild": "rimraf dist",
    "build": "npm-run-all -p build:*",
    "build:esm": "esbuild --sourcemap --target=node16 --format=esm --outdir=dist/esm src/**",
    "build:cjs": "esbuild --target=node16 --format=cjs --sourcemap --outdir=dist/cjs src/**",
    "build:types": "tsc -p .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,yml,yaml}": "prettier --write"
  },
  "prettier": "@djthoms/prettier-config",
  "dependencies": {
    "glob": "^8.0.3",
    "superstruct": "^1.0.3"
  },
  "devDependencies": {
    "@babel/parser": "^7.12.7",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@djthoms/eslint-config": "^5.0.0",
    "@djthoms/prettier-config": "^4.0.0",
    "@docusaurus/types": "^2.0.0-beta.21",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.0.0",
    "esbuild": "^0.17.7",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "npm-run-all": "^4.1.5",
    "react-docgen": "^5.4.2",
    "rimraf": "^4.1.2",
    "semantic-release": "^20.0.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react-docgen": "^5.0.0"
  }
}
